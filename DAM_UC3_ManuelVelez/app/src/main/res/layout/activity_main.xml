            app:layout_constraintBottom_toBottomOf="@+id/textView13"
            app:layout_constraintStart_toEndOf="@+id/textView13"
            app:layout_constraintTop_toTopOf="@+id/textView13">

            <RadioButton
                android:id="@+id/radioButton_Mujer"
                android:layout_width="88dp"
                android:layout_height="37dp"
                android:text="@string/RadioGroup_Mujer"
                android:textSize="12sp" />

            <RadioButton
                android:id="@+id/radioButton_Hombre"
                android:layout_width="wrap_content"
                android:layout_height="38dp"
                android:layout_marginEnd="1dp"
                android:text="@string/RadioGroup_Hombre"
                android:textSize="12sp" />
        </RadioGroup>

        <EditText
            android:id="@+id/Text_Dia"
            android:layout_width="48dp"
            android:layout_height="35dp"
            android:ems="10"
            android:hint="dd"
            android:inputType="date"
            android:text="@string/Text_DiadeNacimiento"
            android:textAlignment="center"
            android:textSize="12sp"
            app:layout_constraintBottom_toBottomOf="@+id/cajablanca3"
            app:layout_constraintEnd_toEndOf="@+id/cajablanca3"
            app:layout_constraintStart_toStartOf="@+id/cajablanca3"
            app:layout_constraintTop_toTopOf="@+id/cajablanca3" />

        <EditText
            android:id="@+id/Text_Mes"
            android:layout_width="78dp"
            android:layout_height="37dp"
            android:ems="10"
            android:hint="mm"
            android:inputType="text"
            android:text="@string/Text_MesdeNacimiento"
            android:textAlignment="center"
            android:textSize="12sp"
            app:layout_constraintBottom_toBottomOf="@+id/cajablanca12"
            app:layout_constraintEnd_toEndOf="@+id/cajablanca12"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="@+id/cajablanca12"
            app:layout_constraintTop_toTopOf="@+id/cajablanca12"
            app:layout_constraintVertical_bias="0.444" />

        <EditText
            android:id="@+id/Text_Año"
            android:layout_width="52dp"
            android:layout_height="35dp"
            android:ems="10"
            android:hint="aaaa"
            android:inputType="date"
            android:text="@string/Text_AñodeNacmiento"
            android:textAlignment="center"
            android:textSize="12sp"
            app:layout_constraintBottom_toBottomOf="@+id/cajablanca11"
            app:layout_constraintEnd_toEndOf="@+id/cajablanca11"
            app:layout_constraintStart_toStartOf="@+id/cajablanca11"
            app:layout_constraintTop_toTopOf="@+id/cajablanca11" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="36dp"
            android:text=" para un famoso, grupo de música o empresa"
            android:textSize="12sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView9" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="360dp"
