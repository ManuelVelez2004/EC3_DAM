            android:inputType="textEmailAddress"
            android:text="@string/Text_CorreoElectronico"
            android:textSize="12sp"
            app:layout_constraintBottom_toTopOf="@+id/checkBox_CerrarSesion"
            app:layout_constraintStart_toStartOf="parent"
